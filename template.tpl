___SANDBOXED_JS_FOR_WEB_TEMPLATE___

// Permissions
const encodeUriComponent = require('encodeUriComponent');
const injectScript = require('injectScript');

// Set values from tag config
const programID = data.hasOwnProperty('programID') ? '' + data.programID : '';
const eventID = data.hasOwnProperty('eventID') ? '' + data.eventID : '';
const referenz = data.hasOwnProperty('referenz') ? '' + data.referenz : '';
const betrag = data.hasOwnProperty('betrag') ? '' + data.betrag : '';

// Set tracking host
const domain = 't.adcell.com';
const path = 't/track.js';

// Construct Script URL
let scriptUrl = 'https://' + domain + '/' + path + '?pid=' + encodeUriComponent(programID) + '&eventid=' + encodeUriComponent(eventID) + '&referenz=' + encodeUriComponent(referenz) + '&betrag=' + encodeUriComponent(betrag);

injectScript(scriptUrl);

// data.gtmOnSuccess aufrufen, wenn das Tag fertig ist.
data.gtmOnSuccess();


